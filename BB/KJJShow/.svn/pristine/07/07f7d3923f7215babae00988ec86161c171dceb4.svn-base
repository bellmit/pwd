            body{
				background: #EAEAEA;
			}
            #all{
				position: absolute;
				top: 0.4rem;
				left: 0;
				width: 100%;
				bottom: 0.45rem;
				overflow: auto;
				-webkit-overflow-scrolling: touch; /* ios5+ */
			}
			.swiper-container{
				width: 100%;
				height: 1.6rem;
				
			}
			.swiper-slide{
				width: 100%;
				height: 100%;
			}
			.swiper-slide img{
				width: 100%;
				height: 100%;
			}
			.nav,.message,.rongshangNews{
				background: #EEEEEE;
				box-shadow:0 0.02rem 0.08rem #D2D2D2;
				
				/*#DCDCDC*/
			}
			.nav{
				width: 100%;
				display: flex;
				
			}
			.nav li {
				width: 24.99%;
				height: 100%;
				border-bottom: 0.01rem solid #E1E1E1;
			}
			.nav li+li {
				border-left: 0.01rem solid #E1E1E1;
			}
			.nav li a{
				width: 100%;
				height: 100%;
				display: flex;
				flex-direction: column;
				text-align: center;
				font-size: 0.12rem;
				font-weight: bold;
				color: #818181;
			}
			.nav li a img{
				width: 0.4rem;
				height: 0.4rem;
				display: block;
				margin: 0.08rem auto 0.05rem;
			}
			
			.message{
				width: 100%;
				margin-top: 0.08rem;
				
			}
			.message p{
				height: 0.35rem;
				line-height: 0.35rem;
				font-size: 0.12rem;
				width: 3rem;
				margin-left: 0.05rem;
				overflow: hidden;
				text-overflow: ellipsis;
				white-space: nowrap;
			}
			.message i{
				color: #54a8b9;
				margin-right: 0.02rem;
			}
			.message span{
				font-size: 0.13rem;
				font-weight: bold;
			}
			
			.rongshangNews{
				margin-top: 0.08rem;
			}
			.rongshangNews-head{
				padding: 0.08rem 0.05rem;
				font-size: 0.14rem;
				line-height: 0.14rem;
				font-weight: bold;
				overflow: hidden;
				
			}
			.rongshangNews-head a{
				float: right;
				font-size: 0.12rem;
				font-weight: normal;		
			}
			.rongshangNews-list{
				padding: 0.05rem;
				margin: 0 0.05rem;
				border-top: 0.01rem solid #D3D3D3;
				overflow: hidden;
				position: relative;
			}
			.rongshangNews-list img{
				width: 1rem;
				height: 0.7rem;
				float: left;
			}
			.rongshangNews-list p{
				width: 1.9rem;
				
				
				
			}
			.rongshangNews-list-title{
				font-size: 0.13rem;
				float: right;
				overflow: hidden;
				text-overflow: ellipsis;
				display: -webkit-box;
				-webkit-line-clamp: 2; /* 这个表示要显示几行 */
				-webkit-box-orient: vertical;
			}
			.rongshangNews-list-con{
				overflow: hidden;
				position: absolute;
				bottom: 0.05rem;
				right: 0;
			}
			.rongshangNews-list-eyes{
				font-size: 0.18rem;
				color: #54a8b9;
				margin-right: 0.05rem;
			}
			.rongshangNews-list-readNum{
				font-size: 0.13rem;
				font-weight: bold;
			}
			.rongshangNews-list-tab{
				float: right;
				font-size: 0.14rem;
				font-weight: bold;
				border: 0.01rem solid #B3B3B3;
				color: #B3B3B3;
				padding:0.01rem 0.15rem;
				border-radius: 0.05rem;
				-webkit-border-radius: 0.05rem;
				-moz-border-radius: 0.05rem;
				
			}
			.rongshangNews-hotTag{
				display: block;
				margin: 0 0.05rem;
				font-size: 0.12rem;
				padding-left: 0.05rem;
				padding-top: 0.05rem;
				border-top: 0.01rem solid #D3D3D3;
			}
			.rongshangNews-nav{
				width: 100%;
				display: flex;
				justify-content: space-around;
				padding: 0.1rem 0;
			}
			.rongshangNews-nav li {
				
				height: 100%;
				
			}
			
			.rongshangNews-nav a{
				width: 100%;
				height: 100%;
				display: flex;
				flex-direction: column;
				
				
				
				
			}
			.rongshangNews-nav img{
				width: 0.5rem;
				height: 0.5rem;
				border-radius: 50%;
				-webkit-border-radius: 50%;
				-moz-border-radius: 50%;
				margin: 0 auto;
				margin-bottom: 0.05rem;
			}
			.rongshangNews-nav p{
				width: 0.8rem;
				overflow: hidden;
				text-overflow: ellipsis;
				white-space: nowrap;
				font-size: 0.12rem;
				text-align: center;
			}