            header{
				position: fixed;
				top: 0;
				left: 0;
				width: 100%;
				height: 0.4rem;
				line-height: 0.4rem;
				background: #1d4894;
				text-align: center;
				color: white;
				font-size: 0.15rem;
				z-index: 20;
			}
			header span{
				color: white;
				font-size: 0.15rem;
			}
			.turnBtn,.searchBtn{
				position: absolute;
			}
			.turnBtn{
				left: 0.05rem;
			}
			.searchBtn{
				right: 0.05rem;
				font-size: 0.12rem;
			}
			.turnBtn i{
				
				
				font-size: 0.2rem;
		
			}
			.searchBtn i{
			
				
				font-size: 0.2rem;
				
			}