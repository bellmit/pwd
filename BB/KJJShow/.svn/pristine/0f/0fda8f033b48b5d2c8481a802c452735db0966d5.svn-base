*{
	font-size: 16px;
	font-family: "微软雅黑";
}
body{
	margin: 0;
	padding: 0;
}
ul,li{
	padding: 0;
	margin: 0;
	list-style-type: none;
}
p{
	padding: 0;
	margin: 0;
}
a{
	text-decoration: none;
	-webkit-tap-highlight-color: transparent;
}
input[type=text],input[type=password],input[type=file],input[type=button],textarea,select,button{
	border: none;
	outline: none;
	appearance: none;
	-webkit-appearance: none;
	-moz-appearance: none;
	
	border-radius: 0;
    -webkit-border-radius: 0;
	-moz-border-radius: 0;
}

