<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_setting_change_account"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/setting_changeaccount_bg"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="rongshanghui.tastebychance.com.rongshanghui.mime.setting.SettingChangeAccountActivity"
    tools:showIn="@layout/activity_setting_change_account">

    <TextView
        android:id="@+id/setting_changeaccount_phone_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/rectangle_grayedge_style"
        android:ems="10"
        android:inputType="phone"
        android:layout_centerVertical="true"
        android:minHeight="40dp"
        android:paddingStart="@dimen/dp_10"
        android:paddingEnd="@dimen/dp_10"
        android:gravity="center_vertical"
        android:textColorHint="@color/lightgray" />

    <TextView
        android:id="@+id/setting_changeaccount_change_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/setting_changeaccount_phone_tv"
        android:layout_alignEnd="@+id/setting_changeaccount_phone_tv"
        android:layout_alignTop="@+id/setting_changeaccount_phone_tv"
        android:gravity="center"
        android:paddingEnd="@dimen/dp_5"
        android:text="变更账户"
        android:textColor="@color/font_blue" />
</RelativeLayout>
