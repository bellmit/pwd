<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <ImageView
        android:id="@+id/hdly_child1__bg_iv"
        android:layout_width="0dp"
        android:layout_height="174dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/pic1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="10dp"
        android:scaleType="fitXY"
        android:layout_marginEnd="10dp"
        android:layout_marginTop="16dp" />

    <TextView
        android:id="@+id/twolevellist_head_tv"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:text="TextView"
        android:gravity="center_vertical"
        android:background="@color/black_transparent"
        app:layout_constraintBottom_toBottomOf="@+id/hdly_child1__bg_iv"
        app:layout_constraintLeft_toLeftOf="@+id/hdly_child1__bg_iv"
        app:layout_constraintRight_toRightOf="@+id/hdly_child1__bg_iv"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:paddingLeft="@dimen/dp_10"
        android:paddingRight="@dimen/dp_10"
        android:lineSpacingExtra="8sp" />

</android.support.constraint.ConstraintLayout>