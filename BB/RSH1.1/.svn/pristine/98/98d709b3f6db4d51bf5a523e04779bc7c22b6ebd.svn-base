<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_register_bmzj"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context="rongshanghui.tastebychance.com.rongshanghui.register.RegisterBMZJEntranceActivity">

    <include
        android:id="@+id/include3"
        layout="@layout/head_includestatusbar" />

    <EditText
        android:id="@+id/register_bmzj_selectareacode_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/rectangle_grayedge_style"
        android:gravity="center"
        android:hint="请输入账户"
        android:inputType="textPersonName"
        android:paddingBottom="@dimen/dp_8"
        android:paddingEnd="@dimen/dp_4"
        android:paddingStart="@dimen/dp_4"
        android:paddingTop="@dimen/dp_8"
        android:textColorHint="@color/gray" />

    <EditText
        android:id="@+id/register_bmzj_getverifycode_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/rectangle_grayedge_style"
        android:gravity="center"
        android:hint="请输入口令"
        android:inputType="textPersonName"
        android:paddingBottom="@dimen/dp_8"
        android:paddingEnd="@dimen/dp_4"
        android:paddingStart="@dimen/dp_4"
        android:paddingTop="@dimen/dp_8"
        android:textColorHint="@color/gray" />
</LinearLayout>
