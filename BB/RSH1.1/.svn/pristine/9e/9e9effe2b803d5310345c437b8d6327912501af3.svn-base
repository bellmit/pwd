<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_zncategory_add"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="rongshanghui.tastebychance.com.rongshanghui.mime.managercommon.zhinan.ZNCategoryAddActivity"
    tools:showIn="@layout/activity_zncategory_add">


    <EditText
        android:id="@+id/activity_zncategory_add_edt"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_marginEnd="42dp"
        android:layout_marginStart="42dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/rectangle_grayedge_style"
        android:gravity="center_vertical"
        android:hint="请输入您要添加的类别名称"
        android:paddingEnd="@dimen/dp_8"
        android:paddingStart="@dimen/dp_8"
        android:textColorHint="@color/lightgray"
        android:textSize="@dimen/sp_15" />
</RelativeLayout>
