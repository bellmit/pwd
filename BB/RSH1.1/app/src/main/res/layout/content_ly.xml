<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_mine_feed_back"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="rongshanghui.tastebychance.com.rongshanghui.zwdt.hdly.ly.LYActivity"
    tools:showIn="@layout/activity_ly">


    <EditText
        android:id="@+id/hdyl_ly_edt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/circle5_grayedge_style"
        android:gravity="top|left"
        android:hint="给福清市长留言..."
        android:imeOptions="actionDone"
        android:lineSpacingExtra="6.0dp"
        android:minHeight="216dp"
        android:inputType="textMultiLine"
        android:padding="10dp"
        android:textColor="@color/text_black"
        android:textColorHint="@color/lightgray"
        android:textSize="17sp" />

</RelativeLayout>
