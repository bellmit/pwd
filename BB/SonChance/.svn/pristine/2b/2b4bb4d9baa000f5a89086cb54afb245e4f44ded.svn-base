public class GiveDetailInfo implements Serializable{
    private String dishes_name;//	赠送菜品名
    private int num;//	赠送数量(0为没有赠送)
    private float save_price;//	赠送等于的优惠价格

    public String getDishes_name() {
        return dishes_name;
    }

    public void setDishes_name(String dishes_name) {
        this.dishes_name = dishes_name;
    }

    public int getNum() {
        return num;
    }

    public void setNum(int num) {
        this.num = num;
    }

    public float getSave_price() {
        return save_price;
    }

    public void setSave_price(float save_price) {
        this.save_price = save_price;
    }
}
