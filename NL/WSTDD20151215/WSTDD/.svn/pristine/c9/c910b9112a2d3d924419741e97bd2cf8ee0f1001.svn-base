<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:peng="http://schemas.android.com/apk/res/com.newland.wstdd"
    android:layout_width="match_parent"
    android:layout_height="match_parent"    
    android:baselineAligned="false"
    android:background="@color/bg_gray"
    android:orientation="vertical" >

   <LinearLayout 
        style="@style/layout_mw_style"
           android:background="@color/bg_gray"
        android:orientation="vertical">
        
       
    <RelativeLayout
        android:id="@+id/registration_edit_listview_item_toprl1"
        android:background="@color/text_red"
        style="@style/layout_mw_style"
        android:paddingTop="10dip"
         android:paddingBottom="10dip"
        android:orientation="horizontal">   
    <TextView
        android:id="@+id/registration_edit_sxtop_name"
        style="@style/layout_ww_style" 
        android:layout_marginLeft="20dip"
        android:layout_alignParentLeft="true"
        android:textColor="@color/white"
        android:layout_centerVertical="true"
        android:text="林多多"/>   
     <TextView
        android:id="@+id/registration_edit_sxtop_phone"
        style="@style/layout_ww_style" 
        android:layout_marginLeft="20dip"
        android:textColor="@color/white"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/registration_edit_sxtop_name"
        android:text="18750736798"/>    

     <TextView
         android:id="@+id/registration_edit_sxtop_delete"
         style="@style/layout_ww_style"
         android:layout_alignParentRight="true"
         android:layout_centerVertical="true"
         android:layout_marginRight="20dip"
         android:background="@drawable/text_rectangle_whiteboder_style"
         android:text="删除"
         
         android:textColor="@color/white" />
 
   </RelativeLayout>
    <RelativeLayout
        android:id="@+id/registration_edit_listview_item_toprl2"
        style="@style/layout_mw_style"
           android:background="@color/white"
        android:orientation="horizontal">   
    <TextView
        style="@style/layout_ww_style" 
        android:layout_marginLeft="20dip"
        android:paddingTop="17dip"
        android:paddingBottom="10dip"
        android:layout_alignParentLeft="true"
        android:text="编辑信息"/>    
     <TextView
        android:id="@+id/registration_edit_sxtop_edit"
        style="@style/layout_ww_style" 
        android:paddingTop="17dip"
        android:layout_marginRight="20dip"
        android:paddingBottom="10dip"
        android:layout_alignParentRight="true"
        android:text="完成"/> 
   </RelativeLayout>
   
   <com.newland.wstdd.find.categorylist.registrationedit.editregistration.EditSxRegistrationEditListViews
                android:id="@+id/registration_sx_listview"
                style="@style/layout_mw_style"
                android:background="@color/white" >
   </com.newland.wstdd.find.categorylist.registrationedit.editregistration.EditSxRegistrationEditListViews>
    
 </LinearLayout>

</LinearLayout>